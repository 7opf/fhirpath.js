{
  "name": "fhirpath",
  "version": "0.6.0",
  "description": "A FHIRPath engine",
  "main": "src/fhirpath.js",
  "dependencies": {
    "antlr4": "^4.7.1",
    "commander": "^2.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0-beta.55",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "jest": "^23.4.1",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "generateParser": "cd src/parser; rm -rf ./generated/*; java -Xmx500M -cp \"../../antlr-4.7.1-complete.jar:$CLASSPATH\" org.antlr.v4.Tool -o generated -Dlanguage=JavaScript FHIRPath.g4",
    "test": "jest && npm run lint",
    "test-converter": "jest converter",
    "build": "webpack",
    "lint": "eslint src/parser/index.js src/*.js",
    "build-converter": "npx babel converter --out-dir ./dist/data"
